{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb",
        "plugin:prettier/recommended",
        "prettier/react"
    ],
    "globals": {
        // "Atomics": "readonly",
        // "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "indent": [2, 2],
        "react/prop-types": 0,
        "prettier/prettier": ["error", {
        }]
    },
    "parser": "babel-eslint"
}